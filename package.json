{
  "name": "create-oursky-react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@oursky/react-scripts": "1.1.1-custom.1",
    "babel-polyfill": "6.26.0",
    "history": "4.7.2",
    "intl": "1.2.5",
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-intl": "2.4.0",
    "react-redux": "5.0.7",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "redux": "4.0.0-beta.2",
    "type-zoo": "1.2.1"
  },
  "devDependencies": {
    "@types/history": "4.6.2",
    "@types/prop-types": "15.5.2",
    "@types/react": "16.0.38",
    "@types/react-dom": "16.0.4",
    "@types/react-intl": "2.3.5",
    "@types/react-redux": "5.0.15",
    "@types/react-router": "4.0.22",
    "@types/react-router-dom": "4.2.4",
    "prettier": "1.10.2",
    "stylelint": "9.1.0",
    "stylelint-config-recommended": "2.1.0",
    "stylelint-no-unsupported-browser-features": "2.0.0",
    "tslint": "5.9.1",
    "tslint-config-prettier": "1.8.0",
    "tslint-react": "3.5.1",
    "typescript": "2.7.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "stylelint": "stylelint --max-warnings 0 'src/**/*.{css,scss}'",
    "prettier": "prettier --list-different 'src/**/*.{js,jsx,ts,tsx,css,scss}'",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,scss}'",
    "tslint": "tslint --project tsconfig.json --config tslint.json --format verbose",
    "tsc": "tsc --noEmit",
    "ci": "yarn run tsc && yarn run tslint && yarn run stylelint && yarn run prettier"
  }
}
